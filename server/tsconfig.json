{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "alwaysStrict": true,
        "esModuleInterop": true,
        "outDir": "./dist",
        "rootDir": "./",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "allowJs": false,
        "allowUnusedLabels": false,
        "allowUnreachableCode": false,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
    },
    "exclude": [
        "node_modules",
    ]
}